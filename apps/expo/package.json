    "@tanstack/zod-form-adapter": "^0.42.1",
    "@trpc/client": "catalog:",
    "@trpc/server": "catalog:",
    "@trpc/tanstack-react-query": "catalog:",
    "better-auth": "1.2.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.5",
    "expo": "54.0.30",
    "expo-constants": "18.0.12",
